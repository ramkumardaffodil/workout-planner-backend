{
    "version": 2,
    "builds": [
      {
        "src": "build/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "build/app.js"
      }
    ],
    "functions": {
      "auth/**/*": {
        "maxDuration": 20
      },
      "workout/**/*": {
        "maxDuration": 20
      }
    }
  }